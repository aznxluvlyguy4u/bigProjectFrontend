{
  "name": "nsfo-online-admin",
  "version": "1.0.0",
  "scripts": {
    "clean": "rm -rf dist",
    "typings": "typings install",
    "lite": "lite-server",
    "build-css": "node-sass --include-path sass src/assets/sass/main.sass src/assets/css/main.css",
    "watch-css": "nodemon -e sass -x \"npm run build-css\"",
    "start": "concurrently \"npm run lite\" \"npm run tsc:w\" \"npm run watch-css\"",
    "start-sans-compiler": "concurrently \"npm run lite\" \"npm run watch-css\"",
    "postinstall": "npm run typings",
    "build": "npm run clean && tsc",
    "build:prod": "npm run build && browserify -s main build/main.js > build/bundle.js && npm run minify",
    "minify": "uglifyjs build/bundle.js --screw-ie8 --compress --mangle --output build/bundle.min.js"
  },
  "license": "ISC",
  "dependencies": {
    "@angular/common": "2.0.0-rc.3",
    "@angular/compiler": "2.0.0-rc.3",
    "@angular/core": "2.0.0-rc.3",
    "@angular/forms": "0.1.1",
    "@angular/http": "2.0.0-rc.3",
    "@angular/platform-browser": "2.0.0-rc.3",
    "@angular/platform-browser-dynamic": "2.0.0-rc.3",
    "@angular/router": "3.0.0-alpha.8",
    "@angular/upgrade": "2.0.0-rc.3",

    "systemjs": "0.19.27",
    "core-js": "^2.4.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "^0.6.12",

    "font-awesome": "4.6.3",
    "foundation-sites": "^6.2.1",
    "jquery": "^2.2.3",
    "lodash": "^4.13.1",
    "moment": "^2.12.0",
    "ng2-pagination": "^0.3.2",
    "ng2-translate": "^2.2.0"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "lite-server": "^2.1.0",
    "node-sass": "^3.4.2",
    "nodemon": "^1.9.1",
    "typescript": "^1.9.0-dev.20160625-1.0",
    "typings": "^1.0.4",
    "uglifyjs": "^2.4.10",
    "browserify": "^13.0.1"
  }
}
